{
    "name": "ts-insp",
    "version": "0.0.2",
    "description": "Tool to visualize typescript dependencies in a project",
    "main": "dist/index.js",
    "author": "Klemeesi",
    "license": "Apache-2.0",
    "tags": [],
    "bin": {
        "ts-insp": "./bin/ts-insp"
    },
    "files": [
        "templates",
        "docs",
        "dist",
        "bin"
    ],
    "types": "./dist/types.d.ts",
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "coverageReporters": [
            "text"
        ]
    },
    "dependencies": {
        "commander": "^12.0.0",
        "node-html-to-image": "^4.0.0",
        "typescript": "^5.3.3",
        "uuidv4": "^6.2.13"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.21",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2"
    },
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc && cp src/types.d.ts dist/types.d.ts",
        "quick": "yarn build && yarn start -c ts-insp.config.ts",
        "test": "jest",
        "test:coverage": "jest --coverage"
    }
}
